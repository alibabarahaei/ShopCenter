<html lang="fa" dir="rtl">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>پروفایل</title>
    <partial name="_HeaderReferencesPartial" />
    @RenderSection("Styles",false)
</head>
<body>

    @await Component.InvokeAsync("DashboardHeader")


    <div class="container-main">
        <div class="d-block">
            <section class="profile-home">
                <div class="col-lg">
                    <div class="post-item-profile order-1 d-block">
                        <div class="col-lg-3 col-md-3 col-xs-12 pr">

                            @await Component.InvokeAsync("SidebarDashboard")
                        </div>
                        <div class=" col-lg-9 col-md-9 col-xs-12 pl">
                        @RenderBody()

                        </div>
                    </div>
                </div>
            </section>
        </div>
    </div>
@await Component.InvokeAsync("DashboardFooter")
<!-- scroll_Progress------------------------->
<div class="progress-wrap">
    <svg class="progress-circle svg-content" width="100%" height="100%" viewBox="-1 -1 102 102">
        <path d="M50,1 a49,49 0 0,1 0,98 a49,49 0 0,1 0,-98" style="transition: stroke-dashoffset 10ms linear 0s; stroke-dasharray: 307.919, 307.919; stroke-dashoffset: 307.919;"></path>
    </svg>
</div>
<!-- scroll_Progress------------------------->
<!-- file js---------------------------------------------------->
<partial name="_FooterRefrencesPartial"/>
@RenderSection("Scripts", false)
    <partial name="_SiteMessagesPartial" />
</body>
</html>